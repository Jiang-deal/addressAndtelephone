{{define "get/index.tmpl"}}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>获取小说信息</title>
    <link crossorigin="anonymous" href="https://cdn.bootcss.com/twitter-bootstrap/3.3.7/css/bootstrap.min.css"
        rel="stylesheet">
    <link rel="stylesheet" href="/xxx/index.css">

    <script crossorigin="anonymous" src="https://cdn.bootcdn.net/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

    

</head>



<body>

    


    <!-- 服务器的地址 action 方法用get获取 {"State":0}-->
        <div class="content">
        
            <h1>输入地址</h1>
			<input type="text" id="ph" placeholder="adderss" />
			
			<button id="btn" >发送</button>
        
        <!-- !得到状态码了 State -1风险  0空号  1实号 -->
        </div>

    

    <script>

       
        var btn = document.getElementById('btn');//绑定发送按钮
        console.log("谢谢")
        btn.onclick = function () {
            console.log("谢谢")

            var phone = $("#ph").val();
            console.log(phone)

            var data1 =
            {
                "Msg": phone,
            };

            var da;
            var sta;
            $.ajax({
            
                //url
                url: 'http://localhost:9090/address',
                data: data1,
                type: 'GET',
                contentType: 'application/json',
                //响应体结果
                dataType: 'json',
                //成功的回调
                success: function (data) {
                    sta=data.msg
                    da=JSON.stringify(data);
                   
                    console.log("状态"+sta);
                    console.log("原神"+da);
                    alert(da);


                     
                   
                     
                    
                },
                //超时时间

                error: function () {
                    
                    console.log('出错啦!!');
                },

            });


           
           
            

        }
    



    </script>
</body>

</html>
{{end}}